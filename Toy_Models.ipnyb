     ]
    },
    {
     "data": {
      "text/plain": [
       "GridSearchCV(cv=5,\n",
       "             estimator=Pipeline(steps=[('preprocessor',\n",
       "                                        ColumnTransformer(transformers=[('skew',\n",
       "                                                                         PowerTransformer(),\n",
       "                                                                         []),\n",
       "                                                                        ('num',\n",
       "                                                                         Pipeline(steps=[('scaler',\n",
       "                                                                                          StandardScaler()),\n",
       "                                                                                         ('num_imputer',\n",
       "                                                                                          IterativeImputer(random_state=0))]),\n",
       "                                                                         ['CRIM',\n",
       "                                                                          'ZN',\n",
       "                                                                          'INDUS',\n",
       "                                                                          'CHAS',\n",
       "                                                                          'NOX',\n",
       "                                                                          'RM',\n",
       "                                                                          'AGE',\n",
       "                                                                          'DIS',\n",
       "                                                                          'RAD',\n",
       "                                                                          'TAX',\n",
       "                                                                          'PTRATIO',\n",
       "                                                                          'B',\n",
