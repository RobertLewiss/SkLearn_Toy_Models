    "#adding nulls\n",
    "for i in range(int(len(df)*0.2)):\n",
    "    df['RM'][randrange(len(df))] = np.nan\n",
    "    df['AGE'][randrange(len(df))] = np.nan\n",
    "    df['TAX'][randrange(len(df))] = np.nan\n",
    "    \n",
    "df.isnull().sum() * 100 / len(df)\n",
    "\n",
    "df.head(10)"
