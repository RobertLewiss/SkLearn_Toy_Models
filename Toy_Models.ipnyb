   "source": [
    "fscore = (2 * p  * r ) / (p  + r )\n",
    "# locate the index of the largest f score\n",
    "ix = np.argmax(fscore)\n",
    "print('Best Threshold=%f, F-Score=%.3f' % (thresholds[ix], fscore[ix]))\n",
    "precision_recall_threshold(p, r, thresholds, 0.566667)"
   ]
