   "source": [
    "#### Split data and create lists"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "ce7029c3-b669-4f00-87ce-92e3d8a1ddd1",
   "metadata": {},
   "outputs": [
    {
